<package>
  <name>fanuc_m20ia_moveit_config</name>
  <version>0.1.0</version>
  <description>
    <p>
      MoveIt package for the Fanuc M-20iA.
    </p>
    <p>
       An automatically generated package with all the configuration and launch 
       files for using the Fanuc M-20iA with the MoveIt Motion Planning 
       Framework.
    </p>
  </description>
  <author email="assistant@moveit.ros.org">MoveIt Setup Assistant</author>
  <maintainer email="g.a.vanderhoorn@tudelft.nl">G.A. vd. Hoorn (TU Delft Robotics Institute)</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/fanuc_m20ia_moveit_config</url>
  <url type="bugtracker">https://github.com/ros-industrial/fanuc_experimental/issues</url>
  <url type="repository">https://github.com/ros-industrial/fanuc_experimental</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>fanuc_m20ia_support</build_depend>

  <run_depend>moveit_ros_move_group</run_depend>
  <run_depend>xacro</run_depend>
  <run_depend>industrial_robot_simulator</run_depend>
  <run_depend>fanuc_m20ia_support</run_depend>
  <run_depend>fanuc_m20ia_moveit_plugins</run_depend>
  <run_depend>moveit_simple_controller_manager</run_depend>

  <export>
    <architecture_independent />
  </export>
</package>
